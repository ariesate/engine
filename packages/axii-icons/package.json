{
  "name": "axii-icons",
  "version": "1.0.2",
  "author": "sskyy <skyking_H@hotmail.com>",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./": "./dist/"
  },
  "scripts": {
    "start": "pnpm start --filter \"axii-icons-playground\"",
    "build": "node ./scripts/build.js",
    "publish": "pnpm run build && bump --commit \"release: axii-icons v%s\" --push && npm publish",
    "postinstall": "pnpm build"
  },
  "dependencies": {
    "@icon-park/svg": "^1.3.1"
  },
  "devDependencies": {
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "command-line-args": "^5.1.1",
    "esbuild": "^0.11.9",
    "rmfr": "^2.0.0"
  },
  "peerDependencies": {
    "axii": "workspace:^1.0.5"
  }
}
