    "axios": "^0.21.1",
    "codeflask": "^1.4.1",
    "esbuild": "^0.11.9",
    "esbuild-plugin-less": "^1.0.5",
    "history": "^5.0.0",
    "moment": "^2.28.0",
    "normalize.css": "^8.0.1",
    "path-to-regexp": "^6.1.0",
    "querystringify": "^2.2.0",
    "tui-grid": "^4.16.1",
    "tui-image-editor": "^3.14.0",
    "x-data-spreadsheet": "^1.0.27"
  },
  "peerDependencies": {
    "axii": "^1.0.4"
  },
  "type": "module",
  "devDependencies": {
    "command-line-args": "^5.1.1",
    "editorjs-table": "^1.4.10",
